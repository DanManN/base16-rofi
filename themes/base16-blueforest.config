! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #1e5c1e, #1e5c1e, #141F2E

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #1e5c1e, #FFCC33, #1e5c1e, #1e5c1e, #375780
rofi.color-active:  #1e5c1e, #a2cff5, #1e5c1e, #1e5c1e, #a2cff5
rofi.color-urgent:  #1e5c1e, #fffab1, #1e5c1e, #1e5c1e, #fffab1

! Set the desired separator style
rofi.separator-style: solid
